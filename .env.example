# API Keys
EXPO_PUBLIC_OPENAI_API_KEY=your_openai_api_key_here

# Map Configuration
EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=your_google_maps_api_key_here

# App Configuration
EXPO_PUBLIC_DEFAULT_ZOOM=12
EXPO_PUBLIC_MAP_PROVIDER=apple  # 'apple', 'google', or 'auto'

# Mapbox Settings
# Public token for frontend usage
EXPO_PUBLIC_MAPBOX_ACCESS_TOKEN=YOUR_MAPBOX_PUBLIC_TOKEN_HERE
# Secret token for build/download (should be in a secure environment variable in production)
EXPO_PUBLIC_MAPBOX_SECRET_TOKEN=YOUR_MAPBOX_SECRET_TOKEN_HERE

# Google Maps (for reference, not used with Mapbox)
# EXPO_PUBLIC_GOOGLE_MAPS_API_KEY=YOUR_GOOGLE_MAPS_API_KEY_HERE

# Other API Keys
# EXPO_PUBLIC_OPENAI_API_KEY=YOUR_OPENAI_API_KEY_HERE

# Build Configuration
ANDROID_PACKAGE_NAME=com.yourusername.mapapp 